{
  "name": "immo",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p . && cp package.json dist/",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts | pino-pretty",
    "npm:update": "npm upgrade --global yarn && ncu -a -u && rm -rf node_modules yarn.lock && yarn",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "yarn tslint:fix && yarn prettier && yarn build",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.10",
    "@types/express": "^4.16.0",
    "@types/lowdb": "^1.0.6",
    "@types/node": "^10.12.18",
    "@types/pino": "^5.8.3",
    "@types/prompts": "^1.2.0",
    "husky": "^1.3.0",
    "nodemon": "^1.18.9",
    "npm-check-updates": "^2.15.0",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/express-jwt": "^0.0.41",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/pino-http": "^4.0.0",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "lowdb": "^1.0.0",
    "pino": "^5.10.5",
    "pino-http": "^4.1.0",
    "prompts": "^2.0.1",
    "uuid": "^3.3.2"
  }
}
