{
  "name": "immo",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p . && cp package.json dist/",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts | pino-pretty",
    "npm:update": "npm upgrade --global yarn && ncu -a -u && rm -rf node_modules yarn.lock && yarn",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "yarn tslint:fix && yarn prettier && yarn build",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.16",
    "@types/express": "^4.17.2",
    "@types/lowdb": "^1.0.6",
    "@types/node": "^13.7.2",
    "@types/pino": "^5.15.5",
    "@types/prompts": "^2.0.3",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "npm-check-updates": "^4.0.1",
    "pino-pretty": "^3.6.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/express-jwt": "^0.0.42",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/pino-http": "^4.3.2",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^3.4.7",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "pino": "^5.16.0",
    "pino-http": "^4.4.0",
    "prompts": "^2.3.1",
    "uuid": "^3.4.0"
  }
}
